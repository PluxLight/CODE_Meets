<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.hypeboy.codemeets.model.dao.UserInfoDao">
	
		<select id="getUserInfoList" parameterType="int" resultType="UserInfoDto">
			<![CDATA[
				select *
				from user_info
				where user_pk = #{userPk}
			]]>
		</select>
	


</mapper>

